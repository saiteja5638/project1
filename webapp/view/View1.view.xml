<mvc:View xmlns:unified="sap.ui.unified"
    xmlns:f="sap.f"
    controllerName="project1.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns="sap.m"
>
    <Page
        id="_IDGenPage"
        title="Sample Dev"
        titleAlignment="Center"
        titleLevel="H1"
    >
<List id="_IDGenList"  items="{/QUOT_HEADER_DATA}">
    <items>
        <ObjectListItem id="_IDGenObjectListItem1" title="{ZQUOTATION}">
            <firstStatus>
                <!-- Use 'text' property instead of 'title' to avoid the colon -->
                <ObjectStatus id="_IDGenObjectStatus" text="{NAME1}" />
            </firstStatus>
            <ObjectAttribute id="_IDGenObjectAttribute" text="{REGIO}" />
            <secondStatus>
                <ObjectStatus text="{ORT01}" />
            </secondStatus>
            
        </ObjectListItem>
    </items>
</List>
    </Page>
</mvc:View>

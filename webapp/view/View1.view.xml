<mvc:View xmlns:unified="sap.ui.unified"
    xmlns:f="sap.f"
    controllerName="project1.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns="sap.m"
>
    <Page
        id="_IDGenPage"
        title="Sample Dev"
        titleAlignment="Center"
        titleLevel="H1"
    >
    <Toolbar id="_IDGenToolbar" >
    <ToolbarSpacer id="_IDGenToolbarSpacer" ></ToolbarSpacer>
    <unified:FileUploader id="_IDGenFileUploader"   
           multiple="false"
        
        change="handleFileChange"  ></unified:FileUploader>
    </Toolbar>
   <Table id="_IDGenTable1"  items="{/JOBlogs}" >
   <infoToolbar>
   <Toolbar id="_IDGenToolbar1" >
   <Button id="_IDGenButton"  text="Submit"  press="handleActionCall" ></Button>
   </Toolbar>
   </infoToolbar>
   <columns>
   <Column id="_IDGenColumn"   >
   <Title id="_IDGenTitle"  text="JOB_ID" ></Title>
   </Column>
   </columns>
   <items>
   <ColumnListItem id="_IDGenColumnListItem" >
   <Text id="_IDGenText"  text="{ID}" ></Text>
   </ColumnListItem>
   </items>
   </Table>
   <VBox id="_IDGenVBox1"   ></VBox>
    </Page>
</mvc:View>
